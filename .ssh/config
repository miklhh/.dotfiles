host liu
    HostName ssh.liu.se
    User mikhe33
    Port 22
host liu-proxy
    HostName ssh.edu.liu.se
    User mikhe33
    Port 22
host only-da-proxy
    HostName only-da.ad.liu.se
    User mikhe33
    Port 22
    ProxyCommand ssh liu-proxy -W %h:%p
    RequestTTY yes
    RemoteCommand zsh
host only-da
    HostName only-da.ad.liu.se
    User mikhe33
    Port 22
    RequestTTY yes
    RemoteCommand zsh
host maokai-proxy
    HostName maokai.staff.netlogon.liu.se
    User mikl
    Port 22
    ProxyCommand ssh liu-proxy -W %h:%p
host maokai
    HostName maokai.staff.netlogon.liu.se
    User mikl
    Port 22

